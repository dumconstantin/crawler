{
  "name": "Crawler",
  "version": "0.1.0",
  "description": "A simple web crawler",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "WEB_PORT=8080 node server.js",
